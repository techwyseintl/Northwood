.dgreen_box
  .dgreen_header
    %div
  .dgreen_content{:style => "padding: 0px 20px"}
    
    = error_messages_for :career_report
    - form_for :career_report, @career_report, :url => career_reports_path, :html => {:multipart => true, :id=>'file_upload_form'} do |f|
      %dl.form_list
        %dt
          %label{:for=>"rate_rate"} File:
        %dd.user_form
          = f.file_field :uploaded_data
        %dt
          %label{:for=>"rate_rate"} Report name:
        %dd.user_form
          = f.text_field :name

      = submit_tag "Create", :class => 'dont_display'    
      .button{:style=>'float:right'}
        = link_to 'Upload File', '#', :onclick => "$('file_upload_form').submit(); return false;"      
      
      .clearfloat
  .dgreen_footer
    %div
      